		//console.log(data_pager);
		var _token = $('meta[name="csrf-token"]').attr('content');
	
		//一列車の場合はTab表示に対応するためコンストラクタ第4引数にTabありを指定する
		var pager = new OneTrainPager(_token,'/timetable/railway/ajax/train/',data_pager,true);
		//----------------------------------------------------------------
		//ページャのコンテンツ読み込みたびに行う初期化処理を設定
		//ajaxで内容を読み込んだ際に独自の初期化が必要な場合の関数を設定しておく
		//----------------------------------------------------------------
		pager.extinit.push(setTabMenu);//タブ設定
		pager.extinit.push(setSlideToggleMenu);//基準駅までの要素の開閉する関数
		//初期化処理実行※spの場合はmaster-jsでsetSlideToggleMenuを実行済みのためpcのみ初期表示時に実行する
					setSlideToggleMenu();
				

		//初期表示時のページャの設定を行う。
		pager.modifyPager(getInput()['tx']);
		
		//pc用のスライド切り替え関数の実行とajax読み込み時の初期化用
		//※spのみはmaster-jsにて定義されている
		function setSlideToggleMenu(){
				//開閉ボタンのDOM取得
				var btn = {
					open: $('.js-slide-toggle-open'),
					close: $('.js-slide-toggle-close')
				};

				//開閉ボタンがある時のみ実行
				if( btn.open.length && btn.close.length ) {
					//開くボタン押下時
					btn.open.on('click', function(e){
						e.preventDefault();
										
						$(this).parents('.js-slide-toggle-tgt').find('.js-slide-toggle-close').addClass('active');
						$(this).parents('.js-slide-toggle-tgt').find('.js-slide-toggle-content').stop().slideDown();
						$(this).removeClass('active');
					});
					//閉じるボタン押下時
					btn.close.on('click', function(e){
						e.preventDefault();
												
						$(this).parents('.js-slide-toggle-tgt').find('.js-slide-toggle-open').addClass('active');
						$(this).parents('.js-slide-toggle-tgt').find('.js-slide-toggle-content').stop().slideUp();
						$(this).removeClass('active');
					});
				}
			};

		//タブ用設定
		function setTabMenu(){
			var tgt_tab = $('.tab-wrap');
			var tgt_tab_menu = tgt_tab.find('.tab-menu li');

			tgt_tab_menu.on('click',function(e){
				e.preventDefault();
				var _this = $(this);
				var index = _this.index();

				if( _this.hasClass('active') ) return false;

				//メニューの切替
				_this.parents('.tab-wrap').find('.tab-menu li').removeClass('active');
				_this.addClass('active');

				//コンテンツの切替
				_this.parents('.tab-wrap').find('.tab-content-inner').removeClass('active').eq(index).addClass('active');
			});
		};
	});
</script>
</body>
</html>
